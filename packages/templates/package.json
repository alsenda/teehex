{
  "name": "@teehex/templates",
  "version": "0.1.0",
  "description": "Template sources for teehex CLI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./index.ts",
  "exports": {
    ".": {
      "types": "./index.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "base",
    "backend-vercel",
    "frontend-angular",
    "frontend-ember",
    "frontend-preact",
    "frontend-react",
    "frontend-solid",
    "frontend-svelte",
    "frontend-style-canonical.css",
    "frontend-vue",
    "overlays",
    "adapters"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "node -e \"import('node:fs').then(fs=>fs.rmSync('dist',{recursive:true,force:true}))\""
  },
  "dependencies": {
    "@teehex/shared": "workspace:*"
  }
}
