{
  "name": "create-hexagon-ts",
  "version": "0.1.0",
  "description": "CLI installer for teehex templates",
  "type": "module",
  "bin": {
    "create-hexagon-ts": "./dist/index.js",
    "teehex": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "node -e \"import('node:fs').then(fs=>fs.rmSync('dist',{recursive:true,force:true}))\""
  },
  "dependencies": {
    "@teehex/shared": "workspace:*",
    "@teehex/templates": "workspace:*"
  }
}
